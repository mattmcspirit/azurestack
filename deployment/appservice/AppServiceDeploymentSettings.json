{
    "AzureResourceManagerDnsSuffix": "adminmanagement.<<customDomainSuffix>>",
    "TenantAzureResourceManagerDnsSuffix": "management.<<customDomainSuffix>>",
    "AzureDirectoryTenantName": "<<AzureDirectoryTenantName>>",
    "IdentityApplicationId": "<<IdentityApplicationId>>",
    "VirtualNetwork": {
        "name": "AppServiceVirtualNetwork",
        "properties": {
            "addressSpace": {
                "addressPrefixes": [
                    "10.200.0.0/16"
                ]
            }
        }
    },
    "ControllersSubnet": {
        "name": "ControllersSubnet",
        "properties": {
            "addressPrefix": "10.200.0.0/24"
        }
    },
    "ManagementServersSubnet": {
        "name": "ManagementServersSubnet",
        "properties": {
            "addressPrefix": "10.200.1.0/24"
        }
    },
    "FrontEndsSubnet": {
        "name": "FrontEndsSubnet",
        "properties": {
            "addressPrefix": "10.200.2.0/24"
        }
    },
    "PublishersSubnet": {
        "name": "PublishersSubnet",
        "properties": {
            "addressPrefix": "10.200.3.0/24"
        }
    },
    "WorkersSubnet": {
        "name": "WorkersSubnet",
        "properties": {
            "addressPrefix": "10.200.8.0/21"
        }
    },
    "SelectedControllerSKUs": {
        "name": "<<controllerSkuSize>>"
    },
    "SelectedManagementRoleSKUs": {
        "name": "<<managementSkuSize>>"
    },
    "SelectedPublisherRoleSKUs": {
        "name": "<<publisherSkuSize>>"
    },
    "SelectedFrontEndRoleSKUs": {
        "name": "<<frontendSkuSize>>"
    },
    "SelectedWorkerSKUs": {
        "name": "<<workerSkuSize>>"
    },
    "SelectedSmallWorkerSKUs": {
        "name": "Standard_A1_v2"
    },
    "SelectedMediumWorkerSKUs": {
        "name": "Standard_A2"
    },
    "SelectedLargeWorkerSKUs": {
        "name": "Standard_A3"
    },
    "FileShareUNCPath": "\\\\<<FileServerDNSLabel>>\\Websites",
    "FileServerSKUs": "Standard_A1",
    "FileShareOwner": "FileShareOwner",
    "FileShareOwnerPassword": "<<Password>>",
    "FileShareUser": "FileShareUser",
    "FileShareUserPassword": "<<Password>>",
    "ResourceGroupName": "appservice-infra",
    "StorageAccountName": "appsvclocalstor",
    "IdentityPfxCertPath": "<<CertPathDoubleSlash>>\\sso.appservice.<<customDomainSuffix>>.pfx",
    "IdentityPfxCertPassword": "<<certPassword>>",
    "RootCertPath": "<<CertPathDoubleSlash>>\\AzureStackCertificationAuthority.cer",
    "DefaultPfxCertPath": "<<CertPathDoubleSlash>>\\_.appservice.<<customDomainSuffix>>.pfx",
    "DefaultPfxCertPassword": "<<certPassword>>",
    "ResourceProviderPfxCertPath": "<<CertPathDoubleSlash>>\\api.appservice.<<customDomainSuffix>>.pfx",
    "ResourceProviderPfxCertPassword": "<<certPassword>>",
    "PublisherPfxCertPath": "<<CertPathDoubleSlash>>\\ftp.appservice.<<customDomainSuffix>>.pfx",
    "PublisherPfxCertPassword": "<<certPassword>>",
    "SQLServerName": "<<SQLServerName>>",
    "SQLSysAdminLogin": "<<SQLServerUser>>",
    "SQLSysAdminPassword": "<<Password>>",
    "SelectedControllerInstances": "<<instances>>",
    "SelectedManagementRoleInstances": "<<instances>>",
    "SelectedPublisherRoleInstances": 1,
    "SelectedFrontEndRoleInstances": 1,
    "SelectedWorkerInstances": 1,
    "PlatformImage": {
        "Publisher": "MicrosoftWindowsServer",
        "Offer": "WindowsServer",
        "name": "2016-Datacenter",
        "Version": "latest"
    },
    "WorkerVirtualMachineAdministrator": "workeradmin",
    "WorkerVirtualMachinePassword": "<<Password>>",
    "OtherVirtualMachineAdministrator": "roleadmin",
    "OtherVirtualMachinePassword": "<<Password>>"
}